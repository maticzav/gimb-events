{
  "public": false,
  "name": "gimb-events",
  "version": 2,
  "alias": "events.gimb.io",
  "builds": [
    { "src": "server/src/index.ts", "use": "@now/node-server" },
    { "src": "public/next.config.js", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/api", "dest": "/server/src/index.ts" },
    { "src": "/.*", "dest": "/public/next.config.js" }
  ],
  "env": {
    "APP_SECRET": "@gimb-events-app-secret",
    "PRISMA_ENDPOINT": "@gimb-events-prisma-endpoint",
    "PRISMA_SECRET": "@gimb-events-prisma-secret",
    "SENDGRID_KEY": "@gimb-events-sendgrid-key"
  },
  "github": {
    "enabled": true,
    "autoAlias": true
  }
}
